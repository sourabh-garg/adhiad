<h1>ADD STATE</h1>


<%= render 'shared/error', obj: @state %>
<%= form_for @state do |f| %>

<div class="form-group">
	<%= f.label :state, "state Name"%>
	<%= f.text_field :state, class: "form-control", placeholder: "Enter State name", autofocus: true, required: true%>
	<br>
</div>
<div class="form-group">
	<%= f.submit class: "btn btn-primary"%>

</div>
<% end %>
  